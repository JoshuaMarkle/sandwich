{
	"manifest_version": 3,
	"name": "Sandwich",
	"description": "Sandwich assignments from many sources",
	"version": "1.0",
	"permissions": ["storage", "alarms", "activeTab", "tabs", "scripting", "webRequest"],
	"host_permissions": [
		"https://www.gradescope.com/*"
	],
	"background": {
		"scripts": [
			"lib/browser-polyfill.js",
			"scripts/background.js"
		],
		"type": "module"
	},
	"action": {
		"default_popup": "pages/popup.html",
		"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"64": "icons/icon64.png",
			"128": "icons/icon128.png"
		}
	},
	"options_page": "pages/dashboard.html",
	"web_accessible_resources": [
		{
			"resources": ["lib/browser-polyfill.js.map"],
			"matches": ["<all_urls>"]
		}
	]
}
