{
	"manifest_version": 3,
	"name": "Gradescope Dashboard",
	"version": "1.0",
	"description": "Fetch and display Gradescope assignments.",
	"permissions": ["storage", "cookies", "activeTab", "alarms", "webRequest"],
	"host_permissions": ["https://www.gradescope.com/*"],
	"background": {
		"scripts": ["background.js"]
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	}
}
